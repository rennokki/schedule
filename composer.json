{
    "name": "rennokki/schedule",
    "description": "Schedule is a package that helps tracking schedules for your models. If you have workers in a company, you can set schedules for them and see their availability though the time.",
    "keywords": [
        "schedule",
        "scheduler",
        "timetable",
        "spatie",
        "opening-hours",
        "schedule",
        "opening",
        "hours"
    ],
    "homepage": "https://github.com/rennokki/schedule",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "rennokki@gmail.com",
            "homepage": "https://twitter.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Schedule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Schedule\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Schedule\\ScheduleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
